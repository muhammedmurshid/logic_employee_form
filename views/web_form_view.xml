    <template id="logic_employee_form_success" name="Customer Form Successfully Submitted">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="container">
                    <div class="col-md-12">
                        <div class="alert alert-success">
                            Success..
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>

</odoo>