<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="menu_appointment" model="website.menu">
            <field name="name">Employee Form</field>
            <field name="url">/employee_form</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence">55</field>
        </record>
    </data>
    <template id="online_appointment_form">
        <t t-call="website.layout">
            <script type="text/javascript" src="refund_logic/static/src/js/submitt_button.js"/>
            <div id="wrap">
                <div class="container">
                    <div class="col-md-12">
                        <form action="/employee_form/submit" method="post">
                            <br></br>
                            <h3 align="center">Employee Form</h3>
                            <br></br>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <div class="form-group col-12 s_website_form_field s_website_form_required"
                                             data-type="char" data-name="Field">
                                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                                <label class="col-form-label col-sm-auto s_website_form_label"
                                                       style="width: 200px">
                                                    <span class="s_website_form_label_content">
                                                        Name:
                                                    </span>
                                                    <span class="s_website_form_mark">
                                                        *
                                                    </span>
                                                </label>
                                                <div class="col-sm">
                                                    <input id="employee_name" type="char"
                                                           class="form-control s_website_form_input"
                                                           name="employee_name" placeholder="Employee name"
                                                           required="required"/>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-12 s_website_form_field s_website_form_required"
                                             data-type="char" data-name="Field">
                                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                                <label class="col-form-label col-sm-auto s_website_form_label"
                                                       style="width: 200px">
                                                    <span class="s_website_form_label_content">
                                                        Phone Number:
                                                    </span>
                                                    <span class="s_website_form_mark">
                                                        *
                                                    </span>
                                                </label>
                                                <div class="col-sm">
                                                    <input id="phone_number" type="char"
                                                           class="form-control s_website_form_input"
                                                           name="phone_number" required="required"/>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-12 s_website_form_field s_website_form_required"
                                             data-type="char" data-name="Field">
                                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                                <label class="col-form-label col-sm-auto s_website_form_label"
                                                       style="width: 200px">
                                                    <span class="s_website_form_label_content">
                                                        Designation:
                                                    </span>
                                                </label>
                                                <div class="col-sm">
                                                    <input id="designation" type="char"
                                                           class="form-control s_website_form_input"
                                                           name="designation" required="required"/>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-12 s_website_form_field s_website_form_required"
                                             data-type="char" data-name="Field">
                                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                                <label class="col-form-label col-sm-auto s_website_form_label"
                                                       style="width: 200px">
                                                    <span class="s_website_form_label_content">
                                                        Date Of Joining:
                                                    </span>
                                                </label>
                                                <div class="col-sm">
                                                    <input id="joining" type="date"
